{
  "name": "sscs-ccd-definitions",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "setup": "cd ccd-e2e-tests && yarn install && yarn webdriverUpdate",
    "lint": "echo lint",
    "test": "NODE_PATH=. mocha test/unit/definitions/**/*.js",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "test:functional": "yarn setup && cd ccd-e2e-tests && yarn test:fullfunctional",
    "start": "node index.js",
    "test:coverage": "echo test:coverage",
    "test:a11y": "echo test:a11y",
    "highLevelDataSetup": "echo 'bash bin/before-data-setup-step.sh benefit $1 && ./gradlew highLevelDataSetup --args=$1 && sh bin/after-data-setup-step.sh' > ./temp.sh && sh ./temp.sh",
    "test:smoke": "echo smoke"
  },
  "dependencies": {
    "@cucumber/cucumber": "^9.5.0",
    "@hmcts/nodejs-healthcheck": "^1.8.0",
    "@hmcts/properties-volume": "^1.0.0",
    "codeceptjs": "^3.5.15",
    "config": "^3.3.9",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "express": "^4.19.2",
    "json5": "^2.2.3",
    "puppeteer": "^22.0.0"
  },
  "devDependencies": {
    "@hmcts/eslint-config": "^1.4.0",
    "chai": "^4.3.8",
    "eslint": "^8.48.0",
    "eslint-plugin-mocha": "^10.1.0",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "otp": "^1.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "sonar-scanner": "^3.1.0"
  },
  "resolutions": {
    "semver": "^7.5.4"
  },
  "packageManager": "yarn@3.5.0"
}
