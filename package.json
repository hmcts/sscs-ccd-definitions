{
  "name": "sscs-ccd-definitions",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.15.0"
  },
  "scripts": {
    "reset-ccd-submodule": "git submodule update --init --recursive --remote && git submodule foreach --recursive git pull origin master && yarn --cwd sscs-ccd-e2e-tests install",
    "setup": "git submodule update --init --recursive --remote && yarn --cwd sscs-ccd-e2e-tests install && yarn --cwd sscs-ccd-e2e-tests transpile && yarn --cwd sscs-ccd-e2e-tests webdriverUpdate",
    "lint": "echo lint",
    "test": "NODE_PATH=. mocha test/unit/definitions/**/*.js",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "test:functional": "yarn --cwd sscs-ccd-e2e-tests test:previewfunctional",
    "start": "node index.js",
    "test:coverage": "echo test:coverage",
    "test:a11y": "echo test:a11y",
    "highLevelDataSetup": "echo 'bash bin/before-data-setup-step.sh benefit $1 && ./gradlew highLevelDataSetup --args=$1 && sh bin/after-data-setup-step.sh' > ./temp.sh && sh ./temp.sh",
    "test:smoke": "echo smoke"
  },
  "dependencies": {
    "@hmcts/nodejs-healthcheck": "1.8.0",
    "@hmcts/properties-volume": "0.0.14",
    "codeceptjs": "^3.3.7",
    "config": "^3.3.8",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "express": "^4.18.2",
    "json5": "^2.2.3",
    "puppeteer": "^19.2.2"
  },
  "devDependencies": {
    "@hmcts/eslint-config": "^1.4.0",
    "chai": "^4.3.6",
    "eslint": "^8.26.0",
    "eslint-plugin-mocha": "^10.1.0",
    "husky": "^2.2.0",
    "lodash": "^4.17.21",
    "mocha": "^10.1.0",
    "otp": "^0.1.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.0",
    "pretty-quick": "^1.10.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "sonar-scanner": "^3.1.0"
  },
  "resolutions": {
    "codeceptjs/**/mocha": "^10.0.0",
    "request/**/http-signature": "^1.3.6",
    "request/**/qs": "^6.11.0",
    "@hmcts/nodejs-healthcheck/**/superagent": "^8.0.6"
  }
}
