{
  "name": "e2e-tests",
  "version": "0.2.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "scripts": {
    "build": "yarn webdriverUpdate && yarn transpile",
    "transpile": "yarn tsc -b",
    "lint": "yarn eslint . && yarn prettier --check .",
    "lint:fix": "yarn prettier --write . && yarn eslint . --fix --quiet",
    "webdriverUpdate": "webdriver-manager update --versions.chrome 112.0.5615.49 --gecko=false",
    "e2e": "yarn test:fullfunctional",
    "test:local": "yarn build && cross-env NODE_ENV=development protractor ./dist/e2e/features.parallel.conf.js --cucumberOpts.tags=~@wip --cucumberOpts.tags=@nightly-test",
    "test:aat": "yarn build && cross-env NODE_ENV=aat protractor ./dist/e2e/features.parallel.conf.js --cucumberOpts.tags=~@wip --cucumberOpts.tags=@nightly-test",
    "test:fullfunctional": "yarn build && cross-env NODE_ENV=test protractor ./dist/e2e/features.parallel.conf.js --cucumberOpts.tags=~@wip --cucumberOpts.tags=@nightly-test",
    "test:previewfunctional": "cross-env NODE_ENV=test protractor ./dist/e2e/features.parallel.conf.js --cucumberOpts.tags=~@wip --cucumberOpts.tags=@preview-test",
    "test:crossbrowser": "cross-env NODE_ENV=test-crossbrowser protractor ./dist/e2e/features.crossbrowser.conf.js --cucumberOpts.tags=~@wip --cucumberOpts.tags='@crossbrowser' --cucumberOpts.tags=~@OnlyIfSaveAndContinueIsEnabled",
    "test:prod": "yarn build && cross-env NODE_ENV=test protractor ./dist/e2e/features.parallel.conf.js --cucumberOpts.tags=~@wip --cucumberOpts.tags=@nightly-test --cucumberOpts.tags=~@issue-decision --cucumberOpts.tags=~@uc --cucumberOpts.tags=~@reinstatement --cucumberOpts.tags=~@bundle",
    "test:a11y": "cross-env NODE_ENV=test protractor ./dist/e2e/features.a11y.conf.js --cucumberOpts.tags=~@wip --cucumberOpts.tags=@a11y",
    "test:nsp": "nsp check"
  },
  "dependencies": {
    "@angular/compiler": "^16.2.4",
    "@angular/core": "^16.2.4",
    "@axe-core/webdriverjs": "^4.7.3",
    "@cucumber/messages": "^22.0.0",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@faker-js/faker": "^8.0.2",
    "@hmcts/eslint-config": "^1.4.0",
    "@hmcts/nodejs-logging": "^4.0.4",
    "@sinonjs/fake-timers": "^11.1.0",
    "@types/chai": "^4.3.6",
    "@types/config": "^3.3.1",
    "@types/jasmine": "^4.3.5",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "axe-core": "^4.8.0",
    "chai": "^4.3.8",
    "chai-as-promised": "^7.1.1",
    "codelyzer": "^6.0.2",
    "config": "^3.3.9",
    "cross-env": "^7.0.3",
    "cucumber": "^6.0.7",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cucumber": "^2.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.5.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jasmine-core": "^5.1.1",
    "jasmine-spec-reporter": "^7.0.0",
    "moment": "^2.29.4",
    "multiple-cucumber-html-reporter": "^3.4.0",
    "path": "^0.12.7",
    "prettier": "^3.0.3",
    "protractor": "^7.0.0",
    "protractor-beautiful-reporter": "^1.3.6",
    "protractor-cucumber-framework": "^9.7.0",
    "protractor-retry": "^2.0.1",
    "puppeteer": "^22.0.0",
    "request-promise": "^4.2.6",
    "rxjs": "^7.8.1",
    "selenium-webdriver": "^4.12.0",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "json-schema": "^0.4.0",
    "xml2js": "^0.5.0",
    "thenify": "^3.3.1",
    "jszip": "^3.8.0"
  },
  "packageManager": "yarn@3.5.0",
  "devDependencies": {
    "@cucumber/cucumber": "^9.5.1"
  }
}
