{
  "name": "sscs-ccd-e2e-tests",
  "version": "0.1.0",
  "engines": {
    "node": ">=18.17.0"
  },
  "private": true,
  "scripts": {
    "build": "yarn webdriverUpdate && yarn transpile",
    "transpile": "yarn tsc -b",
    "lint": "yarn eslint . && yarn prettier --check .",
    "lint:fix": "yarn prettier --write . && yarn eslint . --fix --quiet",
    "webdriverUpdate": "webdriver-manager update --versions.chrome 113.0.5672.63 --gecko=false",
    "e2e": "yarn test:fullfunctional",
    "test:local": "yarn build && cross-env NODE_ENV=development protractor ./dist/e2e/features.parallel.conf.js --cucumberOpts.tags=~@wip --cucumberOpts.tags=@nightly-test",
    "test:aat": "yarn build && cross-env NODE_ENV=aat protractor ./dist/e2e/features.parallel.conf.js --tags \"not @wip and @nightly-test\"",
    "test:fullfunctional": "yarn build && cross-env NODE_ENV=aat protractor ./dist/e2e/features.parallel.conf.js --cucumberOpts.tags=@nightly-test",
    "test:previewfunctional": "yarn build && cross-env NODE_ENV=aat protractor ./dist/e2e/features.parallel.conf.js --cucumberOpts.tags=@preview-test",
    "test:crossbrowser": "yarn build && cross-env NODE_ENV=test-crossbrowser protractor ./dist/e2e/features.crossbrowser.conf.js --cucumberOpts.tags=@crossbrowser",
    "test:prod": "yarn build && cross-env NODE_ENV=test protractor ./dist/e2e/features.parallel.conf.js --cucumberOpts.tags=~@wip --cucumberOpts.tags=@nightly-test --cucumberOpts.tags=~@issue-decision --cucumberOpts.tags=~@uc --cucumberOpts.tags=~@reinstatement --cucumberOpts.tags=~@bundle",
    "test:a11y": "yarn build && cross-env NODE_ENV=test protractor ./dist/e2e/features.a11y.conf.js --cucumberOpts.tags=@a11y",
    "test:nsp": "nsp check"
  },
  "dependencies": {
    "@angular/compiler": "16.1.3",
    "@angular/core": "16.1.3",
    "@axe-core/webdriverjs": "4.7.1",
    "@cucumber/cucumber": "^9.1.2",
    "@cucumber/messages": "22.0.0",
    "@cucumber/pretty-formatter": "1.0.0",
    "@faker-js/faker": "8.0.1",
    "@hmcts/eslint-config": "1.4.0",
    "@hmcts/nodejs-logging": "4.0.4",
    "@sinonjs/fake-timers": "^10.2.0",
    "@types/chai": "4.3.5",
    "@types/config": "3.3.0",
    "@types/jasmine": "4.3.5",
    "@types/node": "20.2.3",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "axe-core": "4.7.1",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "codelyzer": "6.0.2",
    "config": "^3.3.9",
    "cross-env": "7.0.3",
    "cucumber": "^6.0.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-cucumber": "2.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "44.2.4",
    "eslint-plugin-prettier": "4.2.1",
    "jasmine-core": "5.0.0",
    "jasmine-spec-reporter": "7.0.0",
    "moment": "2.29.4",
    "multiple-cucumber-html-reporter": "^3.4.0",
    "path": "0.12.7",
    "prettier": "2.8.8",
    "protractor": "7.0.0",
    "protractor-beautiful-reporter": "1.3.6",
    "protractor-cucumber-framework": "9.2.1",
    "protractor-retry": "2.0.1",
    "puppeteer": "^20.3.0",
    "request-promise": "4.2.6",
    "rxjs": "7.8.1",
    "selenium-webdriver": "4.9.2",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "packageManager": "yarn@3.5.1"
}
